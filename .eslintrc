{
    "env": {
      "commonjs": true,
      "browser": true,
      "es6": true,
      "node": true,
      "serviceworker": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": ["import"],
    "rules": {
      "import/first": ["warn", "DISABLE-absolute-first"],
      "quotes": ["warn", "single"],
      "no-underscore-dangle": ["error", { "allow": ["id", "_id"]}]
    }
  }